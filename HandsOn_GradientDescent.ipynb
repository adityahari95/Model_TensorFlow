      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "w= 5\n",
        "L= []\n",
        "eta= 0.1\n",
        "for i in range(100):\n",
        "  w=w-eta*2*w\n",
        "  L.append(w*w)\n",
        "\n",
        "\n"
      ],
